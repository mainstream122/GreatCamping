				,	HMPG_ADDRESS
				,	RSVTN_TYPE
				,	LATITUDE
				,	LONGITUDE
				,	FAC_TOILET
				,	FAC_SHOWERROOM
				,	FAC_SINK
				,	FAC_STORE
				,	FAC_BBQ
				,	FAC_ELECTRIC
				,	FAC_WIFI
				,	FAC_CARAVAN
				,	FAC_GLAMPING
				,	FAC_TENT
				,	FAC_PET_YN
				,	SUB_REGION_ID
				,	AREA -->
