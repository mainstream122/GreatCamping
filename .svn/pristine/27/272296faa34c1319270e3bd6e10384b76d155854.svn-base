  <mapper namespace="Paging">
  
  	<sql id="header">
  		SELECT  *
		FROM    (
		                SELECT  ROWNUM RNUM
		                        , A.*
		                FROM    (
  	</sql>
  	
  	<sql id="footer">
			  							) A
								<!-- WHERE	ROWNUM %lt;= 20-->
									WHERE ROWNUM <![CDATA[<=]]> #{endNumber}
						)
				WHERE	RNUM >= #{startNumber}
  	</sql>
  
  </mapper>